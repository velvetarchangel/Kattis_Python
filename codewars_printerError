public class Printer {
    
    public static String printerError(String s) {
        int count = 0;
        
        for(int i = 0; i < s.length(); i++){
          if (!(s.charAt(i) >= 'a' && s.charAt(i) <= 'm')) count ++;
          }
        return Integer.toString(count) + "/" + Integer.toString(s.length());
    }
}
